    <!-- http://purl.obolibrary.org/obo/SO_0000233 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/SO_0000233">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/SO_0000673"/>
